\def\mdf@frame@frametitlebackground@first{%
 \ifbool{mdf@repeatframetitle}%
  {%repeating title = true
   \rlap%
    {%
     \mdf@frametitlebackground@default%
     \rule[\dimexpr
             -\mdfboundingboxdepth
